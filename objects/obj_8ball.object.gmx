          <argument>
            <kind>1</kind>
            <string>///Stuff
hsp = dir * movespad;

if (place_meeting(x,y+1, obj_wall))
{
    vsp = -jumpspeed;
    
}
vsp += gravi;
//Prevent falling
if (!place_meeting(x-16,y+1, obj_wall)) 
{
    dir = 1
}
if (!place_meeting(x+16,y+1, obj_wall)) 
{
    dir = -1
}
//Horizontal Collision
if (place_meeting(x+hsp,y, obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
    x += sign(hsp)
    }
    hsp = 0;
    
    dir *= -1
}
x += hsp;
//Vertical Collision
if (place_meeting(x,y+vsp, obj_wall))
{
while (!place_meeting(x,y+sign(vsp),obj_wall))
    {
    y += sign(vsp)
    }
    vsp = 0;
}
y += vsp;
//Moving away from dead end
//if ((place_meeting(x+vsp,y,obj_wall)) and (!place_meeting(x,y+1, obj_wall))) 
//{ 
//tick += 1
//if tick = 5 {
 //   dir *= -1 
//    tick = 0
//    }
//
//}
//Enemy collision
if (place_meeting(x,y,obj_player))
{
    if (obj_player.x &lt; x-16)
    {
        with (obj_player) hsp = -jumpspeed;
        instance_destroy();
    }
    if (obj_player.x &lt; x+16)
    {
        with (obj_player) hsp = jumpspeed;
        instance_destroy();
    }
    else
    {
        with (obj_player) health -= 1;
    }

}

</string>
